{
  "name": "clicker-mobile",
  "version": "0.16.1",
  "author": "Tim Giblin",
  "homepage": "https://tryclicker.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4141",
    "start:release": "ng serve --port 4141 --configuration release",
    "start:host": "ng serve --port 4141 --host local.tryclicker.com",
    "start:wait": "ng serve & wait-on http://localhost:4141",
    "start:static": "npx http-server www --port 4141",
    "build2": "npx ng build --configuration=${ENV:-production}",
    "build": "if [ \"$CI_GIT_REF\" = \"master\" ]; then ENV=production; fi && if [ \"$CI_GIT_REF\" = \"release\" ]; then ENV=release; fi && if [ \"$CI_GIT_REF\" = \"develop\" ]; then ENV=develop; fi && echo $ENV && npx ng build --configuration=$ENV",
    "build:ios:develop": "ng build --configuration=production && npx cap sync ios && npx cap open ios",
    "build:ios:release": "ng build --configuration=release && npx cap sync ios && npx cap open ios",
    "build:ios:prod": "ng build --configuration=production && npx cap sync ios && npx cap open ios",
    "build:android:develop": "ng build --configuration=production  && npx cap sync android && npx cap open android",
    "build:android:release": "ng build --configuration=release && npx cap sync android && npx cap open android",
    "build:android:prod": "ng build --configuration=production && npx cap sync android && npx cap open android",
    "generate:assets": "npx cordova-res && cd resources && sh ./copy.sh",
    "test": "ng test",
    "e2e": "npx cypress open",
    "e2e:headless": "npx cypress run --headless",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^9.1.1",
    "@angular/core": "^9.1.1",
    "@angular/forms": "^9.1.1",
    "@angular/platform-browser": "^9.1.1",
    "@angular/platform-browser-dynamic": "^9.1.1",
    "@angular/router": "^9.1.1",
    "@capacitor/android": "^2.0.1",
    "@capacitor/core": "^2.0.1",
    "@capacitor/ios": "^2.0.1",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ionic-native/core": "^5.23.0",
    "@ionic-native/device": "^5.25.0",
    "@ionic-native/diagnostic": "^5.23.0",
    "@ionic-native/geolocation": "^5.23.0",
    "@ionic-native/pro": "^5.14.0",
    "@ionic-native/splash-screen": "^5.23.0",
