{
  "name": "clicker-mobile",
  "version": "0.10.0",
  "author": "Tim Giblin",
  "homepage": "https://tryclicker.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4100",
    "start:host": "ng serve --port 4100 --host local.tryclicker.com",
    "start:wait": "ng serve --port 4100 & wait-on http://localhost:4100",
    "build": "ng build --configuration=production",
    "build:env": "ng build --configuration=$ENV",
    "build:release": "ng build --configuration=release",
    "build:develop": "ng build --configuration=develop",
    "build:ios:develop": "ng build --configuration=production && npx cap sync ios && npx cap open ios",
    "build:ios:release": "ng build --configuration=release && npx cap sync ios && npx cap open ios",
    "build:ios:prod": "ng build --configuration=production && npx cap sync ios && npx cap open ios",
    "build:android:develop": "ng build --configuration=production  && npx cap sync android && npx cap open android",
    "build:android:release": "ng build --configuration=release && npx cap sync android && npx cap open android",
    "build:android:prod": "ng build --configuration=production && npx cap sync android && npx cap open android",
    "generate:assets": "npx cordova-res",
    "test": "ng test",
    "e2e": "npx cypress open",
    "e2e:headless": "npx cypress run --headless",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@angular/forms": "^8.2.2",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.2",
    "@angular/platform-browser-dynamic": "^8.2.2",
    "@angular/router": "^8.2.2",
    "@capacitor/android": "1.1.1",
    "@capacitor/cli": "^1.1.1",
    "@capacitor/core": "1.1.1",
    "@capacitor/ios": "1.1.1",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-brands-svg-icons": "^5.10.0",
    "@fortawesome/free-regular-svg-icons": "^5.10.0",
    "@fortawesome/free-solid-svg-icons": "^5.10.0",
    "@ionic-native/core": "^5.12.0",
    "@ionic-native/diagnostic": "^5.12.0",
    "@ionic-native/geolocation": "^5.12.0",
    "@ionic-native/pro": "^5.12.0",
    "@ionic-native/splash-screen": "^5.12.0",
    "@ionic-native/status-bar": "^5.12.0",
    "@ionic-native/unique-device-id": "^5.12.0",
    "@ionic/angular": "^4.7.4",
    "@ionic/storage": "^2.2.0",
    "@ngrx/effects": "^8.2.0",
    "@ngrx/store": "^8.2.0",
    "@ngrx/store-devtools": "^8.2.0",
    "@sentry/browser": "^5.7.0",
    "angular-in-memory-web-api": "^0.8.0",
    "cordova-plugin-ionic": "^5.4.4",
    "cordova-plugin-whitelist": "^1.3.4",
    "core-js": "^2.6.5",
    "ios": "0.0.1",
    "jwt-decode": "^2.2.0",
    "jwt-simple": "^0.5.6",
    "moment": "^2.24.0",
    "ng-intercom": "^8.0.0",
    "ngx-auto-unsubscribe": "^3.0.1",
    "ngx-moment": "^3.4.0",
    "ngx-segment-analytics": "^1.4.0",
    "ngx-stripe": "^7.4.3",
    "platform": "1.3.5",
    "rxjs": "~6.5.2",
    "uuid": "^3.3.2",
    "wait-on": "^4.0.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.802.2",
    "@angular-devkit/build-angular": "^0.802.2",
    "@angular-devkit/core": "^8.2.2",
    "@angular-devkit/schematics": "^8.2.2",
    "@angular/cli": "~8.2.2",
    "@angular/compiler": "^8.2.2",
    "@angular/compiler-cli": "^8.2.2",
    "@angular/language-service": "^8.2.2",
    "@ionic/angular-toolkit": "^2.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.29",
    "codelyzer": "~4.5.0",
    "cordova-res": "^0.6.0",
    "cypress": "^4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.0.0",
    "tslint": "~5.12.0",
    "typescript": "~3.5.3"
  },
  "description": "Clicker: Change the Channel"
}
