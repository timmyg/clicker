{
  "name": "clicker-mobile",
  "version": "0.4.0",
  "author": "Tim Giblin",
  "homepage": "https://tryclicker.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4100 --host local.tryclicker.com",
    "build": "ng build --configuration=$ENV",
    "build:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "build:ios:release": "ENV=release npm run build && npx cap sync ios && npx cap open ios",
    "build:ios:prod": "ENV=production npm run build && npx cap sync ios && npx cap open ios",
    "build:android": "npm run build && npx cap sync android && npx cap open android",
    "build:android:release": "ENV=release npm run build && npx cap sync android && npx cap open android",
    "build:android:prod": "ENV=production npm run build && npx cap sync android && npx cap open android",
    "generate:assets": "npx cordova-res",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
