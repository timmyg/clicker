    "@ngrx/effects": "^9.1.0",
    "@ngrx/store": "^9.1.0",
    "@ngrx/store-devtools": "^9.1.0",
    "@sentry/browser": "^5.15.4",
    "@stripe/stripe-js": "^1.8.0",
    "angular-in-memory-web-api": "^0.8.0",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic": "^5.4.6",
    "cordova-plugin-whitelist": "^1.3.4",
    "core-js": "^2.6.11",
    "http-server": "^0.12.1",
    "ios": "0.0.1",
    "jwt-decode": "^2.2.0",
