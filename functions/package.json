  "main": "api.longshoreman.js",
  "scripts": {
    "test": "jest",
    "creds:config:dev": "AWS_KEY=$AWS_KEY_DEV AWS_SECRET=$AWS_SECRET_DEV PROFILE=kickthecan:dev STAGE=dev",
    "creds:config:prod": "AWS_KEY=$AWS_KEY_PROD AWS_SECRET=$AWS_SECRET_PROD PROFILE=kickthecan:prod STAGE=prod",
    "serverless:config": "serverless config credentials --provider aws --key $AWS_KEY --secret $AWS_SECRET -n $PROFILE",
    "deploy": "serverless deploy -v --stage $STAGE --aws-profile $PROFILE",
    "logs:account": "serverless logs --function account --stage $STAGE --aws-profile $PROFILE",
    "invoke:register-device": "serverless invoke local --function account --path test/data/new-account.json"
  },
