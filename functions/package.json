{
  "name": "functions",
  "version": "1.0.0",
  "description": "wrapper project for serverless functions",
  "scripts": {
    "test": "jest --coverage",
    "logs:account": "serverless logs --function account --stage $STAGE --aws-profile $PROFILE",
    "invoke:widget:register": "serverless invoke local --function register --path ../.resources/data/widget.json",
    "invoke:command": "serverless invoke local --function remoteCommand --path resources/data/command.json"
  },
  "dependencies": {
    "serverless-helpers": "git+https://github.com/timmyg/serverless-helpers.git#v0.3.12"
  },
  "devDependencies": {
    "flow-bin": "^0.109.0",
    "serverless-prune-plugin": "^1.4.0",
    "serverless-sentry": "^1.2.0"
  }
}
