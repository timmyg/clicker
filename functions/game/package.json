{
  "name": "game",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx flow-remove-types game.flow.js -o game.js",
    "test": "npm run build && jest --coverage",
    "test:specific": "npm run build && jest -t='soccer'",
    "invoke:getStatus": "serverless invoke local --function getStatus --stage develop --path ../.resources/data/action-url.json",
    "invoke:syncNextFewDays": "serverless invoke local --function syncNextFewDays --stage develop",
    "invoke:syncActive": "serverless invoke local --function syncActive --stage develop",
    "invoke:syncAirtable": "serverless invoke local --function syncAirtable --stage develop",
    "invoke:scoreboard": "serverless invoke local --function scoreboard --stage develop",
    "invoke:health": "serverless invoke local --function health --stage develop",
    "invoke:getGame": "serverless invoke local --function get --path ../.resources/data/game.json --stage develop",
    "invoke:updateAirtableGamesStatus": "serverless invoke local --function updateAirtableGamesStatus --stage develop",
    "invoke:scoreboardLiveUpcoming": "serverless invoke local --function scoreboardLiveUpcoming --stage develop"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "airtable": "^0.8.1",
    "aws-sdk": "^2.580.0",
    "aws-xray-sdk": "^2.5.0",
    "axios": "^0.19.0",
    "axios-retry": "^3.1.2",
    "camelcase-keys": "^6.1.0",
    "dotenv": "^6.2.0",
    "dynamoose": "^1.11.1",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.27",
    "object-mapper": "^6.0.1",
    "raven": "^2.6.4",
    "serverless-helpers": "git+https://github.com/timmyg/serverless-helpers.git#v0.3.12",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "flow-bin": "^0.107.0",
    "flow-remove-types": "^2.107.0",
    "jest": "^24.1.0",
    "serverless": "1.83.0",
    "serverless-domain-manager": "^2.6.13",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-prune-plugin": "^1.4.0",
    "serverless-sentry": "^1.2.0"
  }
}
